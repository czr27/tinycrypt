size_t hex2bin (void *bin, char hex[]) {
    size_t  len, i;
    int     x;
    uint8_t *p=(uint8_t*)bin;

    len = strlen (hex);

    for (i=0; i<len; i++) {
      if (isxdigit((int)hex[i]) == 0) {
        return 0; 
      }
    }

    for (i=0; i<len / 2; i++) {
      sscanf (&hex[i * 2], "%2x", &x);
      p[i] = (uint8_t)x;
    } 
    return len / 2;
} 

void bin2hex(char *s, void *bin, int len) {
    uint8_t *p=bin;
    int j;
    printf("%s=", s);
    for(j=0;j<len;j++) {
      if (!(j&7)) putchar('\n');
      printf(" 0x%02x,", p[j]);
    }
    putchar('\n');
}
  
int main(void) {
    uint8_t data[1024];
    size_t     len, i, j;
    
    for(i=0;i<sizeof(key)/sizeof(char*);i++) {
      len = hex2bin(data, cipher[i]);
      bin2hex("", data, len);
    }
    return 0;
}

